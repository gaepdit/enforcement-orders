{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Enfo.WebApp",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/artifacts/Enfo.WebApp/bin/Debug/net6.0/Enfo.WebApp.dll",
      "args": [],
      "cwd": "${workspaceFolder}/src/Enfo.WebApp",
      "stopAtEntry": false,
      "logging": { "moduleLoad": false },
      "console": "internalConsole",
      "env": {
        "ASPNETCORE_ENVIRONMENT": "Local",
        "ENABLE_TEST_USER": "true"
      },
      //   "serverReadyAction": {
      //     "action": "debugWithChrome",
      //     "pattern": "\\bNow listening on:\\s+(https?://\\S+)"
      //   },
      "launchBrowser": {
        "enabled": true,
        "osx": {
          "command": "/Applications/Firefox Developer Edition.app/Contents/MacOS/firefox",
          "args": "${auto-detect-url}"
        },
        "windows": {
          "command": "\"C:\\Program Files\\Firefox Developer Edition\\firefox.exe\"",
          "args": "${auto-detect-url}"
        }
      }
    }
  ]
}